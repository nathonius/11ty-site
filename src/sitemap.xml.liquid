<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {%- for page in collections['all'] %}
        <url>
            <loc>{{ page.url | absolute }}</loc>
            <lastmod>{{ page.date | date_to_xmlschema }}</lastmod>
        </url>
    {%- endfor %}
</urlset> 